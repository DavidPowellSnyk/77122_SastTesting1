@inject SecurityTesting1.Common.Objects.ConfigSettings ConfigSettings
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="~/lib/jquery-3.6.3/jquery.min.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap-5.2.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/lib/fontawesome-free-6.0.0-web/css/all.min.css">
    <script src="~/lib/popper-2.11.2/popper.min.js"></script>
    <script src="~/lib/bootstrap-5.2.3/js/bootstrap.min.js"></script>
    <script src="~/lib/moment-2.29.1/moment.js"></script>
    <script src="~/lib/moment-duration-format-2.2.2/moment-duration-format.js"></script>
    <script src="~/js/common.js?1"></script>
    @RenderSection("scripts", false)
    @RenderSection("style", false)
    <title>@ViewData["Title"]</title>
</head>
<body>
    <main class="container">
        <h1>@ViewData["Title"]</h1>
        @RenderBody()
    </main>
    <footer class="container">
        <small>Viewing this application as: @(String.IsNullOrWhiteSpace(User?.Identity?.Name) ? "Anonymous" : User.Identity.Name) | Server: @(System.Environment.MachineName) | Running as: @(System.Security.Principal.WindowsIdentity.GetCurrent().Name) | @(@System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription)</small>
    </footer>
</body>
</html>